{
    "clearMocks": true,
    "coveragePathIgnorePatterns": ["\\\\node_modules\\\\"],
    "moduleDirectories": ["node_modules"],
    "moduleFileExtensions": [
        "js",
        "mjs",
        "cjs",
        "jsx",
        "ts",
        "tsx",
        "json",
        "node"
    ],
    "rootDir": "../../",
    "testEnvironment": "jsdom",
    "testMatch": "<rootDir>src/**/*(*.)@(spec|test).[tj]s?(x)"
}
